<!doctype html>
<html>
<head>

    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, user-scalable=no, minimal-ui">

    <title>{{#resume.basics}}{{name}} - {{label}}{{/resume.basics}}</title>

    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.2.0/css/all.css"
          integrity="sha384-hWVjflwFxL6sNzntih27bfxkr27PmbbK/iSvJ+a4+0owXq79v+lsFkW54bOGbiDQ" crossorigin="anonymous">
    <style type="text/css">
            {{{css}}}
    </style>

    {{#if resume.gtm_analytics}}
        <script>
            (function (w, d, s, l, i) {
                w[l] = w[l] || [];
                w[l].push({
                    "gtm.start":
                        new Date().getTime(), event: "gtm.js"
                });
                var f = d.getElementsByTagName(s)[0],
                    j = d.createElement(s), dl = l != "dataLayer" ? "&l=" + l : "";
                j.async = true;
                j.src =
                    "https://www.googletagmanager.com/gtm.js?id=" + i + dl;
                f.parentNode.insertBefore(j, f);
            })(window, document, "script", "dataLayer", '{{resume.gtm_analytics}}');
        </script>
    {{/if}}

</head>
<body class="resume-body">
{{#if resume.gtm_analytics}}
    <!-- Google Tag Manager (noscript) -->
    <noscript>
        <iframe src="https://www.googletagmanager.com/ns.html?id={{resume.gtm_analytics}}" height="0" width="0"
        style="display:none;visibility:hidden"></iframe>
    </noscript>
    <!-- End Google Tag Manager (noscript) -->
{{/if}}

<header id="header" class="resume-header">
    {{#resume.basics}}
    <div class="container">
        <div class="row valign-wrapper">
            <div class="col s3">
                {{#if picture}}
                    <img class="picture" src="{{picture}}">
                {{/if}}
            </div>
            <div class="col s9">
                    {{#name}}
                        <h1 class="name">
                            <span class="text">
                                {{.}}
                            </span>
                        </h1>
                    {{/name}}
                    {{#label}}
                        <h3 class="label">
                            {{.}}
                        </h3>
                    {{/label}}
            </div>
        </div>
    </div>
    {{/resume.basics}}
</header>
<div id="content" class="container resume-content">

    {{#resume.basics}}
        <section id="contact" class="row valign-wrapper resume-section">
            <aside class="col s3 resume-section-label">
                <h3><span class="text">Contact</span></h3>
            </aside>
            <div class="col s8 offset-s1 resume-section-content">
                <div class="row contact-methods">
                    {{#email}}
                        <div class="col l4 m6 s12 contact-method">
                            <div>
                                <span class="email"><a href="mailto:{{.}}">{{.}}</a></span>
                            </div>
                        </div>
                    {{/email}}
                    {{#phone}}
                        <div class="col l4 m6 s12 contact-method">
                            <div>
                                <span class="phone"><a href="tel:{{.}}">{{.}}</a></span>
                            </div>
                        </div>
                    {{/phone}}
                    {{#website}}
                        <div class="col l4 s12 contact-method">
                            <div>
                                <span class="website"><a href="{{.}}">{{.}}</a></span>
                            </div>
                        </div>
                    {{/website}}
                </div>
            </div>
        </section>
        <section id="about" class="row valign-wrapper resume-section">
            <aside class="col s3 resume-section-label">
                <h3><span class="text">About</span></h3>
            </aside>
            <div class="col s8 offset-s1 resume-section-content">
                {{#summary}}
                    <p>{{.}}</p>
                {{/summary}}
            </div>
        </section>
        {{#if profiles.length}}
            <section id="profiles" class="row valign-wrapper resume-section">
                <aside class="col s3 resume-section-label">
                    <h3><span class="text">Profiles</span></h3>
                </aside>
                <div class="col s8 offset-s1 resume-section-content">
                    <div class="row social-links">
                        {{#profiles}}
                            <div class="col l4 m6 s12 social-link">
                                {{#if network}}
                                    <i class="fab fa-{{network}}"></i>&nbsp;
                                {{else}}
                                    <i class="fas fa-link"></i>&nbsp;
                                {{/if}}
                                {{#if username}}
                                    <span class="username">
                                        {{#if url}}
                                            <span class="url">
                                                <a href="{{url}}">{{username}}</a>
                                            </span>
                                        {{else}}
                                            {{username}}
                                        {{/if}}
                                    </span>
                                {{else}}
                                    {{#if url}}
                                        <span class="url">
                                            <a href="{{url}}">{{url}}</a>
                                        </span>
                                    {{/if}}
                                {{/if}}
                            </div>
                        {{/profiles}}
                    </div>
                </div>
            </section>
        {{/if}}
    {{/resume.basics}}

    {{#if resume.work.length}}
        <section id="work" class="row resume-section">
            <aside class="col s3 resume-section-label">
                <h3><span class="text">Work</span></h3>
            </aside>
            <div class="col s8 offset-s1 resume-section-content">
                <div class="row">
                    {{#each resume.work}}
                        <div class="col s12">
                            <div>
                                <h5 class="right hide-on-small-only">
                                    <span class="date">{{date startDate}} — {{#if endDate}}{{date endDate}}{{else}}
                                        Present{{/if}}</span>
                                </h5>
                                <h4>
                                    <span class="text">
                                        {{#if website}}
                                            <a href="{{website}}">{{company}}</a>
                                        {{else}}
                                            {{company}}
                                        {{/if}}
                                    </span>
                                </h4>
                                <h5 class="hide-on-med-and-up">
                                    <span class="date">{{date startDate}} — {{#if endDate}}{{date endDate}}{{else}}
                                        Present{{/if}}</span>
                                </h5>
                            </div>
                            <div>
                                {{#website}}
                                    <div class="website right hide-on-small-only">
                                        <a href="{{.}}">{{.}}</a>
                                    </div>
                                {{/website}}
                                {{#position}}
                                    <h5 class="position">
                                        <span class="text">{{.}}</span>
                                    </h5>
                                {{/position}}
                            </div>
                            {{#summary}}
                                <div class="summary">
                                    <p>{{.}}</p>
                                </div>
                            {{/summary}}
                            {{#if highlights.length}}
                                <ul class="highlights">
                                    {{#highlights}}
                                        <li class="bullet">{{.}}</li>
                                    {{/highlights}}
                                </ul>
                            {{/if}}
                        </div>
                    {{/each}}
                </div>
            </div>
        </section>
    {{/if}}

    {{#if resume.skills.length}}
        <section id="skills" class="row resume-section">
            <aside class="col s3 resume-section-label">
                <h3><span class="text">Skills</span></h3>
            </aside>
            <div class="col s8 offset-s1 resume-section-content">
                <div class="row">
                    {{#each resume.skills}}
                        <div class="col s6">
                            {{#name}}
                                <div class="name">
                                    <h5><span class="text">{{.}}</span></h5>
                                </div>
                            {{/name}}
                            {{#if keywords.length}}
                                <ul class="keywords">
                                    {{#keywords}}
                                        <li>{{.}}</li>
                                    {{/keywords}}
                                </ul>
                            {{/if}}
                        </div>
                    {{/each}}
                </div>
            </div>
        </section>
    {{/if}}

    {{#if resume.education.length}}
        <section id="education" class="row resume-section">
            <aside class="col s3 resume-section-label">
                <h3><span class="text">Education</span></h3>
            </aside>
            <div class="col s8 offset-s1 resume-section-content">
                <div class="row">
                    {{#each resume.education}}
                        <div class="col s12">
                            <div>
                                <h5 class="right hide-on-small-only">
                                    <span class="date">{{date startDate}} — {{date endDate}}</span>
                                </h5>
                                <h4>
                                    <span class="text">{{institution}}</span>
                                </h4>
                                <h5 class="hide-on-med-and-up">
                                    <span class="date">{{date startDate}} — {{date endDate}}</span>
                                </h5>
                            </div>
                            <div>
                                {{#area}}
                                    <div class="area right hide-on-small-only">
                                        {{.}}
                                    </div>
                                    <div class="area hide-on-med-and-up">
                                        {{.}}
                                    </div>
                                {{/area}}
                                {{#studyType}}
                                    <div class="studyType">
                                        {{.}}
                                    </div>
                                {{/studyType}}
                            </div>
                            {{#if courses.length}}
                                <h4><span class="text">Highlights</span></h4>
                                <ul class="courses">
                                    {{#courses}}
                                        <li>{{.}}</li>
                                    {{/courses}}
                                </ul>
                            {{/if}}
                        </div>
                    {{/each}}
                </div>
            </div>
        </section>
    {{/if}}

    {{#if resume.awards.length}}
        <section id="awards" class="row resume-section">
            <aside class="col s3 resume-section-label">
                <h3><span class="text">Awards</span></h3>
            </aside>
            <div class="col s8 offset-s1 resume-section-content">
                <div class="row">
                    {{#each resume.awards}}
                        <div class="col s12">
                            <h4>
                                <span class="text">{{title}}</span>
                            </h4>
                            <div class="date right">
                                <em>Awarded</em>
                                {{date date}}
                            </div>
                            {{#awarder}}
                                <div class="awarder">
                                    <em>by</em>
                                    <strong>{{.}}</strong>
                                </div>
                            {{/awarder}}
                            {{#summary}}
                                <div class="summary">
                                    {{.}}
                                </div>
                            {{/summary}}
                        </div>
                    {{/each}}
                </div>
            </div>
        </section>
    {{/if}}

    {{#if resume.volunteer.length}}
        <section id="volunteer" class="row resume-section">
            <aside class="col s3 resume-section-label">
                <h3><span class="text">Volunteer</span></h3>
            </aside>
            <div class="col s8 offset-s1 resume-section-content">
                <div class="row">
                    {{#each resume.volunteer}}
                        <div class="col s12">
                            <h5 class="right hide-on-small-only">
                                <span class="date">{{date startDate}} — {{date endDate}}</span>
                            </h5>
                            <h4>
                                <span class="text">{{organization}}</span>
                            </h4>
                            <h5 class="hide-on-med-and-up">
                                <span class="date">{{date startDate}} — {{date endDate}}</span>
                            </h5>
                            {{#website}}
                                <div class="website right hide-on-small-only">
                                    <a href="{{.}}">{{.}}</a>
                                </div>
                                <div class="website hide-on-med-and-up">
                                    <a href="{{.}}">{{.}}</a>
                                </div>
                            {{/website}}
                            {{#position}}
                                <div class="position">
                                    <span class="text">{{.}}</span>
                                </div>
                            {{/position}}
                            {{#summary}}
                                <div class="summary">
                                    <p>{{.}}</p>
                                </div>
                            {{/summary}}
                            {{#if highlights.length}}
                                <p>
                                    <strong class="text">Highlights</strong>
                                </p>
                                <ul class="highlights">
                                    {{#highlights}}
                                        <li class="bullet">{{.}}</li>
                                    {{/highlights}}
                                </ul>
                            {{/if}}
                        </div>
                    {{/each}}
                </div>
            </div>
        </section>
    {{/if}}

    {{#if resume.publications.length}}
        <section id="publications" class="row resume-section">
            <aside class="col s3 resume-section-label">
                <h3><span class="text">Publications</span></h3>
            </aside>
            <div class="col s8 offset-s1 resume-section-content">
                <div class="row">
                    {{#each resume.publications}}
                        <div class="col s12">
                            <h5 class="right hide-on-small-only">
                                <span class="date">{{releaseDate}}</span>
                            </h5>
                            <h4>
                                <span class="text">{{name}}</span>
                            </h4>
                            <h5 class="hide-on-med-and-up">
                                <span class="date">{{releaseDate}}</span>
                            </h5>
                            {{#website}}
                                <div class="website right hide-on-small-only">
                                    <a href="{{.}}">{{.}}</a>
                                </div>
                                <div class="website hide-on-med-and-up">
                                    <a href="{{.}}">{{.}}</a>
                                </div>
                            {{/website}}
                            {{#publisher}}
                                <div class="publisher">
                                    <em class="text">Published by</em>
                                    <strong>{{.}}</strong>
                                </div>
                            {{/publisher}}
                            {{#summary}}
                                <div class="summary">
                                    <p><span class="text">{{.}}</span></p>
                                </div>
                            {{/summary}}
                        </div>
                    {{/each}}
                </div>
            </div>
        </section>
    {{/if}}

    {{#if resume.languages.length}}
        <section id="languages" class="row valign-wrapper resume-section">
            <aside class="col s3 resume-section-label">
                <h3><span class="text">Languages</span></h3>
            </aside>
            <div class="col s8 offset-s1 resume-section-content">
                <div class="row languages">
                    {{#each resume.languages}}
                        <div class="col l4 m6 s12 language">
                            <div class="language">
                                <strong class="text">{{language}}</strong>&nbsp;<span class="fluency">{{fluency}}</span>
                            </div>
                        </div>
                    {{/each}}
                </div>
            </div>
        </section>
    {{/if}}

    {{#if resume.interests.length}}
        <section id="interests" class="row resume-section">
            <aside class="col s3 resume-section-label">
                <h3><span class="text">Interests</span></h3>
            </aside>
            <div class="col s8 offset-s1 resume-section-content">
                <div class="row">
                    {{#each resume.interests}}
                        <div class="col s6">
                            {{#name}}
                                <div class="name">
                                    <h5><span class="text">{{.}}</span></h5>
                                </div>
                            {{/name}}
                            {{#if keywords.length}}
                                <ul class="keywords">
                                    {{#keywords}}
                                        <li>{{.}}</li>
                                    {{/keywords}}
                                </ul>
                            {{/if}}
                        </div>
                    {{/each}}
                </div>
            </div>
        </section>
    {{/if}}

    {{#if resume.references.length}}
        <section id="references" class="row resume-section">
            <aside class="col s3 resume-section-label">
                <h3><span class="text">References</span></h3>
            </aside>
            <div class="col s8 offset-s1 resume-section-content">
                <div class="row">
                    {{#each resume.references}}
                        <div class="col s12">
                            {{#if reference}}
                                <blockquote class="reference">
                                    <p><span class="text">{{reference}}</span></p>
                                    {{#name}}
                                        <p class="name">
                                            <strong>— <span class="text">{{.}}</span></strong>
                                        </p>
                                    {{/name}}
                                </blockquote>
                            {{/if}}
                        </div>
                    {{/each}}
                </div>
            </div>
        </section>
    {{/if}}
</div>

{{#if resume.ga_analytics}}
    <script>
        (function (i, s, o, g, r, a, m) {
            i["GoogleAnalyticsObject"] = r;
            i[r] = i[r] || function () {
                (i[r].q = i[r].q || []).push(arguments)
            }, i[r].l = 1 * new Date();
            a = s.createElement(o),
                m = s.getElementsByTagName(o)[0];
            a.async = 1;
            a.src = g;
            m.parentNode.insertBefore(a, m)
        })(window, document, "script", "https://www.google-analytics.com/analytics.js", "ga");

        ga("create", '{{resume.ga_analytics}}', "auto");
        ga("send", "pageview");
    </script>
{{/if}}
</body>
</html>

  "scripts": {
    "pretest": "npx gulp -LLLL --color lint",
    "test": "npx gulp -LLLL --color test",
    "postinstall": "npm run build",
    "start": "npm run dev",
    "dev": "npm run dev:client",
    "dev:client": "rm -rf dist/*.js && npx webpack-serve --hotClient",
    "build": "npx gulp -LLLL --color build && npm run resume",
    "resume": "npm run resume:pdf && npm run resume:html",
    "resume:pdf": "npx resume export dist/index.pdf -f pdf --theme randytarampi --force",
    "resume:html": "npx resume export dist/index.html -f html --theme randytarampi --force"
  },

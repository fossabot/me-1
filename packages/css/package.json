{
  "name": "@randy.tarampi/css",
  "description": "Some common styles for www.randytarampi.ca",
  "version": "3.4.1",
  "author": {
    "name": "Randy Tarampi",
    "email": "rt@randytarampi.ca"
  },
  "bugs": "https://github.com/randytarampi/me/issues",
  "dependencies": {
    "@fortawesome/fontawesome-free": "~5.7.0",
    "materialize-css": "~0.100.2"
  },
  "devDependencies": {
    "@babel/cli": "latest",
    "gulp": "~4.0.0"
  },
  "engines": {
    "node": ">=8"
  },
  "files": [
    "npm-shrinkwrap.json",
    "styles"
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": "randytarampi/me.git",
  "scripts": {
    "build": "gulp -LLLL --color build",
    "clean": "gulp -LLLL --color clean",
    "prepack": "if [ -z \"$RELEASE\" ] && [ -z \"$CI\" ]; then NODE_ENV=prd npm run build; fi; if [ ! -d \"./dist\" ]; then exit 74; fi;",
    "prepare": "if [ -z \"$IS_PUBLISHING\" ]; then npm run build; fi;",
    "prepublishOnly": "npm shrinkwrap",
    "pretest": "gulp -LLLL --color lint",
    "preuninstall": "npm run clean"
  }
}

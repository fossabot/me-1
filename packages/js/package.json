{
  "name": "@randy.tarampi/js",
  "description": "Some common JS entities for www.randytarampi.ca",
  "version": "6.1.11",
  "author": {
    "name": "Randy Tarampi",
    "email": "rt@randytarampi.ca",
    "url": "https://www.randytarampi.ca"
  },
  "bugs": "https://github.com/randytarampi/me/issues",
  "dependencies": {
    "@randy.tarampi/schema-dot-org-types": "latest",
    "dms-conversion": "^3.1.1",
    "georaptor": "github:randytarampi/georaptor#me-release",
    "haversine": "^1.1.1",
    "immutable": "github:randytarampi/immutable-js#me-release",
    "latlon-geohash": "^1.1.0",
    "libphonenumber-js": "^1.7.29",
    "lodash": "^4.17.15",
    "luxon": "^1.21.3",
    "proximityhash": "github:randytarampi/proximityhash#me-release",
    "query-string": "^6.9.0"
  },
  "directories": {
    "lib": "./src/lib",
    "test": "./test"
  },
  "files": [
    "es5",
    "esm",
    "npm-shrinkwrap.json"
  ],
  "keywords": [
    "bear",
    "emoji",
    "subtegmine",
    "woof",
    "ʕ•ᴥ•ʔ",
    "υφάδι",
    "гав",
    "نسيج",
    "پود",
    "বুনন",
    "横糸",
    "纬",
    "씨"
  ],
  "license": "MIT",
  "main": "./es5/index.js",
  "module": "./esm/index.js",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "directory": "me/packages/js",
    "type": "git",
    "url": "https://github.com/randytarampi/me.git"
  },
  "scripts": {
    "build": "yarn run build:babel",
    "build:babel": "yarn run build:babel:esm && yarn run build:babel:es5",
    "build:babel:es5": "BABEL_ENV=client.es5 babel src --out-dir es5 --root-mode upward --copy-files",
    "build:babel:esm": "BABEL_ENV=client.esm babel src --out-dir esm --root-mode upward --copy-files",
    "clean": "gulp -LLLL --color clean",
    "cover": "rm -rf coverage/ .nyc_output/; NODE_ENV=test nyc gulp -LLLL --color test",
    "prepack": "if [ -z \"$RELEASE\" ] && [ -z \"$CI\" ]; then NODE_ENV=prd yarn run build; fi; if [ ! -d \"./es5\" ] && [ ! -d \"./esm\" ]; then exit 74; fi;",
    "prepare": "if [ -z \"$IS_PUBLISHING\" ]; then yarn run build; fi;",
    "prepublish": "yarn run snyk-protect",
    "pretest": "gulp -LLLL --color lint",
    "preuninstall": "yarn run clean",
    "snyk-protect": "snyk protect",
    "test": "NODE_ENV=test gulp -LLLL --color test"
  },
  "sideEffects": false,
  "snyk": true
}

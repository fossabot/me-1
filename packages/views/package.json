{
  "name": "@randy.tarampi/views",
  "publishConfig": {
    "access": "public"
  },
  "author": {
    "name": "Randy Tarampi",
    "email": "rt@randytarampi.ca",
    "url": "https://www.randytarampi.ca"
  },
  "version": "0.40.9",
  "description": "Some common Pug templates for www.randytarampi.ca",
  "homepage": "https://www.randytarampi.ca",
  "bugs": {
    "url": "https://github.com/randytarampi/me/issues",
    "email": "bugs@randytarampi.ca"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/randytarampi/me.git"
  },
  "license": "MIT",
  "scripts": {
    "pretest": "gulp -LLLL --color lint",
    "build": "npm run build:gulp && npm run build:babel",
    "build:babel": "BABEL_ENV=publish babel src --out-dir build --root-mode upward --copy-files",
    "build:gulp": "gulp -LLLL --color build",
    "prepare": "if [ -z \"$CI\" ]; then npm run build; fi;",
    "prepack": "NODE_ENV=prd npm run build",
    "clean": "gulp -LLLL --color clean",
    "preuninstall": "npm run clean"
  },
  "dependencies": {
    "@randy.tarampi/js": "^0.40.9",
    "config": "^2.0.1"
  },
  "devDependencies": {
    "@babel/cli": "latest",
    "gulp": "^4.0.0"
  },
  "engines": {
    "node": ">=8"
  },
  "main": "./index.js",
  "esm": "./index.js",
  "directories": {
    "lib": "./lib"
  },
  "files": [
    "build",
    "templates",
    "dist",
    "index.js"
  ]
}

    "@babel/plugin-transform-classes": "latest",
    "@babel/plugin-transform-proto-to-assign": "latest",
    "@babel/polyfill": "latest",
    "@babel/preset-env": "latest",
    "@babel/preset-react": "latest",
    "@babel/register": "latest",
    "babel-loader": "next",
    "babel-plugin-istanbul": "next",
